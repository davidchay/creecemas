<h3 class="h3 mb-0 text-uppercase">{{empresa}}</h3>
<p class="eslogan">{{eslogan}}</p>

<p class="mt-3">
    <small>{{direccion}}</small>
    <small>{{colonia}}, {{codigopostal}}<br></small>
    <small>{{ciudad}}</small>
</p>

<ol>
    <li><a href="tel:{{telefono}}"><i class="fas fa-phone-alt"></i>{{telefono}}</a></li>
    <li><a href="tel:{{celular}}"><i class="fas fa-mobile"></i> {{celular}}</a></li>
    <li><a href="{{urlWhatsapp}}" target="_blank"><i class="fab fa-whatsapp"></i>{{whatsapp}}</a></li>
    <li><a href="mailto:{{email}}" target="_blank"><i class="far fa-envelope"></i>{{email}}</a></li>
    <li><a href="{{urlfacebook}}" target="_blank"><i class="fab fa-facebook"></i>{{facebook}}</a></li>
</ol>